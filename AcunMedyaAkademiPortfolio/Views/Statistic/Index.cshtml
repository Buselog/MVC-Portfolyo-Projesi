
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

@*Linq Sorgular*@

<div class="alert alert-info">Kategori Sayısı: @ViewBag.categoryCount</div>
<div class="alert alert-danger">Proje Sayısı: @ViewBag.projectCount</div>
<div class="alert alert-light">Yetenek Sayısı: @ViewBag.skillCount</div>
<div class="alert alert-success">Ortalama Yetenek Puanı: @ViewBag.skillAvgValue</div>
<div class="alert alert-primary">Son Eklenen Yetenek: @ViewBag.getLastSkillTitle</div>

<div class="alert alert-info">Eklenen Sosyal Medya Hesabı Sayısı: @ViewBag.SocialMediaCount</div>
<div class="alert alert-success">Eklenen Yetenek Sayısı: @ViewBag.HobbyCount</div>
<div class="alert alert-primary">Son Eklenen Projenin Adı: @ViewBag.GetLatProjectName</div>
<div class="alert alert-danger">Son Eklenen Sosyal Medya Hesabı: @ViewBag.GetLastSocialMediaName</div>
<div class="alert alert-info">Son Eklenen Referans Adı: @ViewBag.GetLastTestimonialName</div>



